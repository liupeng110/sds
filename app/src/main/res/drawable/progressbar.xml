<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="5.0dip" />
            <gradient android:startColor="@color/seek_bar_background" android:endColor="@color/seek_bar_background" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/seek_bar_second_progress" android:endColor="@color/seek_bar_second_progress" android:angle="270.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/seek_bar_progress" android:endColor="@color/seek_bar_progress" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>